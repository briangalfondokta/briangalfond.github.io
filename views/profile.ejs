    <div class="container bg-light text-dark">
      <br>
      <div class="row">

        <div class="col-1">
        </div>

        <div class="col-10">

          <form method="POST" id="profileForm" action="/profileUpdate" name="profileForm">

            <!-- Read Only - username & ID -->
            <div class="form-row">
              <div class="form-group col-md-6">
                <label for="inputEmail4">Username:</label>
                <input type="email" class="form-control" name="username" id="username" disabled value= <%= username %> >
              </div>
              <div class="form-group col-md-6">
                <label for="inputPassword4">Okta User ID:</label>
                <input type="email" name="user" id="user"  class="form-control" disabled value= <%= userID %> >
              </div>
            </div>
          <br>

            <!-- Hidden to pass in post -->
          <input name="userID" id="userID"  class="form-control"  hidden value= <%= userID %> >
          <input name="aT" id="aT"  class="form-control"  hidden value= <%= aT %> >


          <div class="form-row">
            <div class="form-group col-md-6">
              <label for="inputEmail4">Phone Number:</label>
              <input class="form-control" name="phone" id="phone" value= <%= phone %> >
            </div>
            <div class="form-group col-md-6">
              <label for="inputPassword4">Backup Email:</label>
              <input type="email" name="secondEmail" id="secondEmail"  class="form-control"  value= <%= secondEmail %> >
            </div>
          </div>

            <div class="form-group">
              <label for="inputAddress">Address:</label>
              <input type="text" class="form-control" name="address" id="inputAddress" value= <%= address %> >
            </div>
            <div class="form-row">
              <div class="form-group col-md-6">
                <label for="inputCity">City:</label>
                <input type="text" class="form-control" name="city" id="inputCity" value= <%= city %> >
              </div>
              <div class="form-group col-md-4">
                <label for="inputState">State:</label>
                <input name="state" id="inputState" class="form-control" value= <%= state %> >
              </div>
              <div class="form-group col-md-2">
                <label for="inputZip">Zip:</label>
                <input type="text" class="form-control" name="zip" id="inputZip" value= <%= zip %> >
              </div>
            </div>
            <br>
            <input type="submit" value="Update Profile">
            <br>
          </form>

        </div>
        <div class="col-1">
        </div>
      </div>
      <br>
    </div>
